{
  "name": "skill-asset-manager",
  "description": "CLI to upload and synchronize skill assets",
  "version": "1.0.5",
  "main": "dist/index.js",
  "bin": {
    "asset": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "test:l": "mocha -r ts-node/register test/setup.ts test/specs/*.ts",
    "test": "LOG_LEVEL=error npm run test:l",
    "test:d": "LOG_LEVEL=debug npm run test:l",
    "test:e2e": "mocha -r ts-node/register test/setup.ts test/specs-e2e/*.ts",
    "check": "LOG_LEVEL=info ts-node ./src/cli sync all alice -c test/assets.config.test.js",
    "help": "ts-node ./src/cli sync --help",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "release": "npm run lint && npm t && npm run build && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint --ext .ts"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "console-log-level": "^1.4.1",
    "fast-glob": "^3.2.7",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/console-log-level": "^1.4.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@types/node-fetch": "^2.5.12",
    "@types/sinon": "^10.0.2",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-mocha": "^9.0.0",
    "lint-staged": "^11.1.2",
    "mocha": "^9.1.1",
    "sinon": "^11.1.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.2.4"
  },
  "author": {
    "name": "Vitaliy Potapov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/skill-asset-manager.git"
  },
  "license": "MIT"
}
